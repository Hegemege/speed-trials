{
  "name": "speed-trials-server",
  "description": "",
  "version": "1.0.0",
  "nyc": {
    "exclude": [
      "**/*.spec.js",
      "node_modules",
      "test"
    ]
  },
  "scripts": {
    "clean": "rm -rf node_modules && rm -rf dist",
    "prod": "cross-env NODE_ENV=production node dist/api.js",
    "dev": "cross-env NODE_ENV=dev nodemon scripts/server.js",
    "build-prod": "rimraf dist && cross-env NODE_ENV=production webpack --config build/webpack.api.config.js --progress --profile --colors",
    "lint": "eslint . --fix"
  },
  "dependencies": {
    "bluebird": "3.5.1",
    "body-parser": "^1.20.1",
    "chalk": "^2.0.1",
    "cookie-parser": "1.4.3",
    "cookie-session": "2.0.0-beta.3",
    "cors": "2.8.4",
    "express": "4.18.2",
    "express-session": "1.15.6",
    "express-socket.io-session": "1.3.2",
    "express-validator": "4.3.0",
    "i18n": "^0.8.3",
    "moment": "^2.18.1",
    "nedb-core": "3.0.6",
    "nedb-promise": "^2.0.1",
    "nedb-session-store": "1.1.2",
    "passport": "0.4.0",
    "passport-oauth": "1.0.0",
    "request": "2.83.0",
    "uuid": "3.2.1",
    "validator": "9.4.0",
    "socket.io": "2.0.4",
    "utf-8-validate": "4.0.0",
    "bufferutil": "3.0.3"
  },
  "devDependencies": {
    "eslint": "^4.3.0",
    "nodemon": "^1.11.0",
    "rimraf": "^2.6.0",
    "cross-env": "^1.0.7",
    "webpack": "3.11.0",
    "stats-webpack-plugin": "0.6.2"
  }
}
